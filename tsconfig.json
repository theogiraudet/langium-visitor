{
    "exclude": ["node_modules", "dist", "examples"],
    "compilerOptions": {
      "declaration": true,
      "target": "ESNext",
      "module": "Node16",
      "moduleResolution": "node16",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,  
      "strict": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "noEmit": true
    }
}